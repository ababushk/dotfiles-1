	<unique-id>1465189473946686-1</unique-id>
	<command>notify-send &quot;Sending to Imgur&quot; %F -i gpicview; imgurbash2 %F &amp;&amp; notify-send &quot;Imgur Copied&quot; &quot;%F: `xclip-output`&quot; -i gpicview || notify-send &quot;Imgur Failed&quot; %F -i gpicview</command>
	<description>Send image to imgur and copy to clipboard</description>
	<patterns>*</patterns>
	<image-files/>
</action>
<action>
	<icon>retext</icon>
	<name>Send to sprunge</name>
	<unique-id>1465617141606559-1</unique-id>
	<command>notify-send &quot;Sending to Sprunge&quot; %F -i retext; cat %F | sprunge | xclip -selection clipboard &amp;&amp; notify-send &quot;Sprunge copied&quot; &quot;%F: `xclip-output`&quot; -i retext || notify-send &quot;Sprunge fails&quot; %F -i retext</command>
	<description>Send files to sprunge and copy link to clipboard: a online file view</description>
	<patterns>*</patterns>
	<other-files/>
	<text-files/>
